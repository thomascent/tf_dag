<launch>
    <group>
        <remap from="/tf" to="/tf_dag"/>
        <node name="world_to_marker" pkg="tf" type="static_transform_publisher" args="0.1 0 0.3 0 0 0 1 world marker 1"/>
        <node name="camera_to_marker" pkg="tf" type="static_transform_publisher" args="0 0.5 0 0 0 0 1 camera marker 1"/>
    </group>

    <node name="tf_dag" pkg="tf_dag" type="tf_dag" output="screen"/>

</launch>
